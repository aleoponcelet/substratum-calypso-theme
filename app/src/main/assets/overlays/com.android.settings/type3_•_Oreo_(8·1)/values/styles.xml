<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SettingsBase" parent="@*android:style/Theme.DeviceDefault.Settings.Dark.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:listDivider">@*android:color/transparent</item>
        <item name="android:colorSecondary">@*android:color/transparent</item>
        <item name="android:colorPrimary">@*android:color/transparent</item>
        <item name="android:colorPrimaryDark">@*android:color/transparent</item>
        <item name="android:colorControlActivated">@*android:color/accent_material_dark</item>
        <item name="android:colorEdgeEffect">@*android:color/highlighted_text_dark</item>
        <item name="android:colorControlHighlight">@*android:color/highlighted_text_dark</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title" parent="@*com.android.settings:style/TextAppearance.AppCompat.Title">
        <item name="android:textSize">@*com.android.settings:dimen/abc_text_size_title_material_toolbar</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="Theme.DeviceDefault.Light.DarkActionBar" parent="@android:style/Theme.Material" >
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowShowWallpaper">true</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
    </style>
    <style name="FallbackHome" parent="@*android:style/Theme.DeviceDefault" />
    <style name="PreferenceFragmentListSinglePane" parent="@*android:style/PreferenceFragmentList">
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="*com.android.settings:colorControlHighlight">@*android:color/highlighted_text_dark</item>
    </style>
    <style name="Theme.DateTimeSettingsSetupWizard" parent="@*android:style/Theme.Material.NoActionBar" />
    <style name="Theme.SettingsLicenseActivity" parent="@*android:style/Theme.Material.Panel" />
    <style name="Theme.AppPicker" parent="@*android:style/Theme.Material.Dialog" />
    <style name="Theme.AlertDialog" parent="@android:style/Theme.Material.Dialog.Alert"/>
    <style name="Theme.SettingsDialog" parent="@android:style/Theme.Material.Dialog.NoActionBar" />
    <style name="Theme.SettingsAlertDialog" parent="@android:style/Theme.Material.Dialog.Alert" />
    <style name="Theme.DialogWhenLarge" parent="@*android:style/Theme.DeviceDefault.Settings.Dark.NoActionBar">
        <item name="android:actionBarStyle">@*com.android.settings:style/Theme.ActionBar</item>
        <item name="*com.android.settings:preferenceBackgroundColor">@*android:color/transparent</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorForeground">@android:color/transparent</item>
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:colorAccent">@*android:color/material_blue_grey_900</item>
        <item name="android:colorPrimary">@android:color/transparent</item>
        <item name="android:colorPrimaryDark">@android:color/transparent</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>

    <style name="FingerprintHeaderStyle" parent="@*android:style/TextAppearance.Material.Subhead">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:paddingTop">16.0dip</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>
    <style name="TextAppearance.SuwDescription" parent="@*com.android.settings:style/TextAppearance.AppCompat.Medium">
        <item name="android:textSize">@*com.android.settings:dimen/suw_description_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>

    <style name="SetupWizardAccessibilitySwitchBarTheme" parent="@*com.android.settings:style/ThemeOverlay.SwitchBar.Settings">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:colorControlActivated">@*android:color/accent_material_light</item>
        <item name="*com.android.settings:switchBarBackgroundColor">@*android:color/button_material_dark</item>
    </style>
    <style name="ThemeOverlay.SwitchBar.Settings" parent="@*android:style/Theme.DeviceDefault.Settings.Dark.NoActionBar">
        <item name="switchBarBackgroundColor">?android:colorSecondary</item>
        <item name="switchBarMarginEnd">@*com.android.settings:dimen/switchbar_subsettings_margin_end</item>
        <item name="switchBarMarginStart">@*com.android.settings:dimen/switchbar_subsettings_margin_start</item>
    </style>
    <style name="Theme.ConfirmDeviceCredentialsDark" parent="@*android:style/Theme.DeviceDefault.Settings.Dark.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
        <item name="android:windowAnimationStyle">@*com.android.settings:style/ConfirmDeviceCredentialsAnimationStyle</item>
        <item name="*com.android.settings:confirmDeviceCredentialsSideMargin">32.0dip</item>
        <item name="*com.android.settings:confirmDeviceCredentialsTopMargin">32.0dip</item>
    </style>
    <style name="GlifTheme.Light" parent="@*com.android.settings:style/SuwThemeGlif">
        <item name="*android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="*android:alertDialogTheme">@*com.android.settings:style/ThemeOverlay.AlertDialog</item>
        <item name="*android:lockPatternStyle">@*com.android.settings:style/LockPatternStyle.Setup</item>
        <item name="*android:preferencePanelStyle">@*android:style/PreferencePanel.Dialog</item>
        <item name="*com.android.settings:fingerprint_layout_theme">@*com.android.settings:style/FingerprintLayoutTheme</item>
        <item name="*com.android.settings:ic_menu_moreoverflow">@*android:drawable/ic_menu_moreoverflow_material</item>
        <item name="*com.android.settings:ic_wps">@*com.android.settings:drawable/ic_wps_light</item>
        <item name="*com.android.settings:preferenceBackgroundColor">@*android:color/background_dark</item>
        <item name="*com.android.settings:preferenceTheme">@*com.android.settings:style/PreferenceTheme.SetupWizard</item>
        <item name="*com.android.settings:setup_divider_color">@*com.android.settings:color/setup_divider_color_light</item>
        <item name="*com.android.settings:side_margin">0.0dip</item>
        <item name="*com.android.settings:wifi_friction">@*com.android.settings:drawable/wifi_friction</item>
        <item name="*com.android.settings:wifi_signal">@*com.android.settings:drawable/wifi_signal</item>
        <item name="*com.android.settings:wifi_signal_color">@*com.android.settings:color/setup_wizard_wifi_color_light</item>
    </style>
    <style name="TextAppearance.CategoryTitle" parent="@*android:style/TextAppearance.Material.Small">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@*android:color/accent_material_dark</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.1</item>
    </style>

    <style name="TextAppearance.CategoryTitleText" parent="@android:style/TextAppearance.Material.Small">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0.1</item>
    </style>

    <style name="Theme.ActionBar" parent="@android:style/Widget.Material.ActionBar.Solid">
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:elevation">0.0dp</item>
    </style>

    <style name="TextAppearance.TileTitle" parent="@android:style/TextAppearance.Material.Medium">
        <item name="android:textSize">14.5sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="CardView" parent="@*com.android.settings:style/Base.CardView">
        <item name="*com.android.settings:cardBackgroundColor">@*android:color/transparent</item>
    </style>
</resources>
